{
  "name": "sander0542/plesk-dns-sync",
  "description": "Sync the Plesk DNS to Cloudflare DNS",
  "keywords": [
    "plesk",
    "cloudflare",
    "dns",
    "sync",
    "api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Sander Jochems",
      "email": "contact@sanderjochems.com",
      "homepage": "https://www.sanderjochems.com",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/Sander0542/DNS-Sync-for-Cloudflare",
  "config": {
    "vendor-dir": "plib/vendor"
  },
  "autoload": {
    "psr-0": {
      "Composer_": [
        "modules/dns-sync-cloudflare/library/",
        "plib/library/"
      ]
    },
    "psr-4": {
      "PleskExt\\Composer\\": [
        "modules/dns-sync-cloudflare/library/Composer/",
        "plib/library/Composer/"
      ]
    }
  },
  "require-dev": {
    "plesk/pm-api-stubs": "^17.0"
  },
  "require": {
    "cloudflare/sdk": "^1.1",
    "plesk/api-php-lib": "^1.0"
  }
}
